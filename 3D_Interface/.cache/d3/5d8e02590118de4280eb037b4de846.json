order-bottom-left-radius: 35px;\n    border-top-left-radius: 35px;\n    transition-duration: 200ms;\n    font-size: 50px;\n    color: #474747;\n    \n}\n.Container.dest{\n    grid-template-rows: auto ;\n    grid-template-columns: auto auto;\n    position:absolute;\n    height: 8%;\n    width: 12%;\n    padding-left: 10px;\n    left: 85.5%;\n    top: 89%;\n    background-color: rgba(10, 10, 10, 0.2);\n} \n\n.Container.camera{\n\n    position:absolute;\n    left: 93%;\n    top: 50%;\n    transform: translate(-50%, -50%);\n    height: 80px;\n    width : 80px;\n    border-radius: 35px;\n}\n\n.Container.pc{\n\n    position:absolute;\n    grid-template-rows: auto ;\n    grid-template-columns: 19% auto auto;\n    left: 83%;\n    top: 1%;\n    height: 5%;\n    width : 16%;\n    font-size: 200%;\n    border-radius: 10px;\n    color: #07c948;\n    border-bottom-left-radius: 35px;\n    border-top-left-radius: 35px;\n    background-color: rgba(10, 10, 10, 0.8);\n}\n\n.camera:hover{\n    height:150px;\n    width: 150px;\n    border-radius: 75px;\n    background-color: rgba(10, 10, 10, 1);\n    color: rgb(0, 253, 97);\n}\n\n.camera:hover > .fa-chevron-up{\n    font-size: 30px;\n}\n.camera:hover > .fa-chevron-down{\n    font-size: 30px;\n} \n\n.fa-camera{\n    grid-area: 2 / 2 ;  \n}\n\n.fa-chevron-up{\n    transition: 600ms ;\n    font-size: 0px;\n    grid-area: 1 / 2 ;\n}\n\n.fa-chevron-down{\n    transition: 600ms;\n    font-size: 0px;\n    grid-area: 3 / 2 ;\n}\n\n.fa-map-marker-alt{\n    transition: 200ms;\n    transition-timing-function: cubic-bezier();\n}\n\n.fa-map-marker-alt:hover{\n  color: #00ffc8;\n}\n\n.fa-map-marker-alt.active{\n    color: #ff0055;;\n    box-shadow: 0px 0px 1px rgba(15 0, 248, 243, 1);\n    font-size: 115%;\n}\n\n.fa-backspace{\n    transition: 200ms;\n    transition-timing-function: cubic-bezier();\n}\n.fa-backspace:active{\n    transition: 0ms;\n    font-size:90%;\n}\n.fa-backspace:hover{\n    color:yellow;\n}\n.fa-globe-americas{\n    /* position: absolute; */\n    left: 10px;\n}\n.text1{\n    justify-self: start;\n    font-size: 50%;\n    color: #9bb8b1;\n}\n.online{\n    justify-self: start;\n    font-size: 50%;\n    color: #21f873;\n}"},"lineCount":null}},"error":null,"hash":"a2aa6f4e406dd9beacc2812e7362025f","cacheData":{"env":{}}}