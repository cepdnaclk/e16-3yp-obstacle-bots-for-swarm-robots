":"@font-face{\n    font-family: \"roboto\";\n    src: url('../fonts/RobotoCondensed-Regular.ttf');\n}\n\n.body {\n    font-family: roboto;\n    background-color: #1b1b1b;\n    margin: 0px;\n}\n\n\n\n.basicLayout .controls {\n    display: grid;\n    grid-template-rows: 20% 20% 20% 20%;\n}\n\n.view {\n    height: 100vh;\n    width: 100vw;\n    background-color: green;\n    overflow: hidden;\n    /* border-bottom-right-radius: 30px; */\n}\n\n.button {\n    height: 50px;\n    width: 200px;\n    color: #00ffc8;\n    position: relative;\n    -webkit-appearance: none;\n    transform: translate(-50%, -50%);\n    top: 50%;\n    left: 50%;\n    background-color: #242424;\n    box-shadow: -2px -2px 5px rgba(100, 91, 90, 0.5),\n                 2px 2px 5px rgba(10, 10, 10, 0.5);\n    border-radius: 25px;\n    border: 2px solid #414141;\n    outline: none;\n    transition-duration: 200ms;\n    transform-origin: 10px;\n}\n\n.button:hover {\n    border: 2px solid #00ffc8;\n}\n\n\n\n.Container{\n    display: grid;\n    grid-template-columns: auto auto auto;\n    grid-template-rows: auto auto auto;\n    background-color: rgba(10, 10, 10, 0.5);\n    align-items: center;\n    justify-items: center;\n    border-bottom-left-radius: 35px;\n    border-top-left-radius: 35px;\n    transition-duration: 200ms;\n    font-size: 50px;\n    color: #474747;\n    \n}\n.Container.dest{\n    grid-template-rows: auto ;\n    grid-template-columns: auto auto;\n    position:absolute;\n    height: 8%;\n    width: 12%;\n    padding-left: 10px;\n    left: 85.5%;\n    top: 89%;\n    background-color: rgba(10, 10, 10, 0.2);\n} \n\n.Container.camera{\n\n    position:absolute;\n    left: 93%;\n    top: 50%;\n    transform: translate(-50%, -50%);\n    height: 80px;\n    width : 80px;\n    border-radius: 35px;\n}\n\n.Container.pc{\n\n    position:absolute;\n    grid-template-rows: auto ;\n    grid-template-columns: 19% auto auto;\n    left: 83%;\n    top: 1%;\n    height: 5%;\n    width : 16%;\n    font-size: 200%;\n    border-radius: 10px;\n    color: #9da7a1;\n    border-bottom-left-radius: 35px;\n    border-top-left-radius: 35px;\n    background-color: rgba(10, 10, 10, 0.8);\n}\n\n.Container.batt{\n\n    position:absolute;\n    grid-template-rows: auto ;\n    grid-template-columns:  auto;\n    left: 83%;\n    top: 80%;\n    height: 5%;\n    width : 7%;\n    transform: translate(-50%, -50%);\n    font-size: 100%;\n    border-radius: 50px;\n    color: #9da7a1;\n    background-color: rgba(10, 10, 10, 0.8);\n    transition-duration: 20ms;\n}\n.Container.batt:hover{\n    \n    background-color: rgba(26, 27, 27, 0.8);\n}\n.Container.batt.active{\n    color: #23cf68;\n    height: 6%;\n    width : 8%;\n}\n\n\n.camera:hover{\n    height:150px;\n    width: 150px;\n    border-radius: 75px;\n    background-color: rgba(10, 10, 10, 1);\n    color: rgb(0, 253, 97);\n}\n\n.camera:hover > .fa-chevron-up{\n    font-size: 30px;\n}\n.camera:hover > .fa-chevron-down{\n    font-size: 30px;\n} \n\n.fa-camera{\n    grid-area: 2 / 2 ;  \n}\n\n.fa-chevron-up{\n    transition: 600ms ;\n    font-size: 0px;\n    grid-area: 1 / 2 ;\n}\n\n.fa-chevron-down{\n    transition: 600ms;\n    font-size: 0px;\n    grid-area: 3 / 2 ;\n}\n\n.fa-map-marker-alt{\n    transition: 200ms;\n    transition-timing-function: cubic-bezier();\n}\n\n.fa-map-marker-alt:hover{\n  color: #00ffc8;\n}\n\n.fa-map-marker-alt.active{\n    color: #ff0055;;\n    box-shadow: 0px 0px 1px rgba(15 0, 248, 243, 1);\n    font-size: 115%;\n}\n\n.fa-backspace{\n    transition: 200ms;\n    transition-timing-function: cubic-bezier();\n}\n.fa-backspace:active{\n    transition: 0ms;\n    font-size:90%;\n}\n.fa-backspace:hover{\n    color:yellow;\n}\n.fa-globe-americas{\n    /* position: absolute; */\n    left: 10px;\n}\n.text1{\n    justify-self: start;\n    font-size: 50%;\n    color: #9bb8b1;\n}\n.online{\n    justify-self: start;\n    font-size: 50%;\n    color: #9da7a1;\n}\n\n\n\n\n.lableContainer{\n    display: grid;\n    position:absolute;\n    grid-template-rows:  70% 30%;\n    left: 500px;\n    top: 500px;\n    height: 4%;\n    width : 4%;\n    font-size: 200%;\n    border-radius: 5px;\n    color: #e8ecea;\n    transform: translate(-50%, -120%);\n    background-color: rgba(0, 8, 8, 0.185);\n    z-index: 2;\n    pointer-events: none;\n    align-items: center;\n    opacity: 0;\n    transition: opacity 0.5s;\n}\n\n\n.labelText{\n    font-size: 60%;\n    color:#38fa99\n}\n.batterybar{\n    display: grid;\n    grid-template-rows: auto;\n    height:100%;\n    background-color: rgba(230, 223, 223, 0.966);\n    border-radius: 4px;\n    align-items: center;\n    overflow: hidden;\n    \n}   \n\n.batterybarChild{\n    height: 100%;\n    width: 50%;    \n    /* border-radius: 100px; */\n    background-color: rgba(85, 248, 35, 0.966);\n    transition: width 1s\n}"},"lineCount":null}},"error":null,"hash":"6d07e80263c4248220b5f5ba24698c56","cacheData":{"env":{}}}